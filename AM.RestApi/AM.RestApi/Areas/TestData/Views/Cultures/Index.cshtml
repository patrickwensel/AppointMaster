@model IEnumerable<AM.VetData.Data.Culture>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
	<tr>
		<th>
			@Html.DisplayNameFor(model => model.CultureCode)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.CountryCode)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.LanguageCode)
		</th>
		<th></th>
	</tr>

@foreach (var item in Model) {
	<tr>
		<td>
			@Html.DisplayFor(modelItem => item.CultureCode)
		</td>
		<td>
			@Html.DisplayFor(modelItem => item.CountryCode)
		</td>
		<td>
			@Html.DisplayFor(modelItem => item.LanguageCode)
		</td>
		<td>
			@Html.ActionLink("Edit", "Edit", new {id = item.CultureCode}) |
			@Html.ActionLink("Details", "Details", new {id = item.CultureCode}) |
			@Html.ActionLink("Delete", "Delete", new {id = item.CultureCode})
		</td>
	</tr>
}

</table>
